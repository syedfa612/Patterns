public class Pattern
{
    public static void pt()
    {
             for(int i=0;i<5;i++)
        {
            for(int j=0;j<5;j++)
            {
                System.out.print("*"+" ");
            }
            System.out.println();
        }
    }
    public static void main(String[] args)
    {
        pt();
   
    }
}

